{
  "name": "purescript-halogen-svg-elems",
  "description": "A collection of SVG elements for Halogen.",
  "repository": {
    "type": "git",
    "url": "https://github.com/MitchStevens/purescript-halogen-svg-elems.git"
  },
  "keywords": [
    "halogen",
    "svg"
  ],
  "type": "module",
  "scripts": {
    "build": "spago build",
    "build-watch": "fd . -e purs | entr -s 'npm run build'",
    "build-svg-spec-tests": "spago build -p svg-specification-tests && spago bundle -p svg-specification-tests",
    "build-svg-spec-tests-watch": "fd . -e purs | entr -s 'npm run build-svg-spec-tests'",
    "test": "spago test",
    "test-watch": "fd . -e purs | entr -s 'npm run test'",
    "migrate": "spago --migrate",
    "format": "purs-tidy format-in-place src test"
  },
  "devDependencies": {
    "esbuild": "^0.21.4",
    "purescript": "^0.15.15",
    "spago": "next"
  },
  "dependencies": { }
}
